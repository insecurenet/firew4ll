msgid "Monthly"
msgstr "Mensilmente"

#: src/usr/local/www/system_advanced_firewall.php:576
msgid "Weekly"
msgstr "Settimanale"

#: src/usr/local/www/system_advanced_firewall.php:577
msgid "Daily"
msgstr "Giornaliero"

